import React from "react";

function FruitList() {
  const fruits = ["Apple", "Banana", "Cherry"]; // Example array

  return (
    <div>
      <h1>Fruit List</h1>
      <ul>
        {fruits.map((fruit, index) => (
          <li key={index}>{fruit}</li> // Use correct variable names
        ))}
      </ul>
    </div>
  );
}

export default FruitList;

