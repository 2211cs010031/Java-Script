    <h1 id="a" class="a">iam abhi</h1>
    <h1 id="b" class="b">iam ram</h1>
    <h1 id="c" class="a">iam ravi</h1>
    <h1 class="a">iam kavya</h1>
    <script>
      let x = document.getElementByTagName("a");
      console.log(x);

      for (i = 0; i < x.length; i++) {
        console.log(x[i]);
      }
    </script>
  </body>
</html>
